{
  "name": "GraphQL-devtools",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Devtools panel for GraphQL development",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
     "16": "images/graphql_16x16.png",
     "48": "images/graphql_48x48.png",
    "128": "images/graphql_128x128.png"
  }
}
